{
  "name": "client-app",
  "version": "0.0.0",
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8000  ./node_modules/@angular/cli/bin/ng build",
    "ng": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json --marker TRANSLATE",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/lang_{en,de,fr,ja,cn}.json --clean --format json --marker TRANSLATE"
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/test-setup.ts",
      "jest-expect-message"
    ]
  },
  "dependencies": {
    "@angular/animations": "^18.2.2",
    "@angular/cdk": "^18.2.2",
    "@angular/common": "^18.2.2",
    "@angular/compiler": "^18.2.2",
    "@angular/core": "^18.2.2",
    "@angular/forms": "^18.2.2",
    "@angular/platform-browser": "^18.2.2",
    "@angular/platform-browser-dynamic": "^18.2.2",
    "@angular/router": "^18.2.2",
    "@ant-design/icons": "^5.4.0",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@egjs/hammerjs": "^2.0.17",
    "@microsoft/signalr": "^8.0.7",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@sentry/browser": "8.27.0",
    "@xivapi/angular-client": "^2.0.1",
    "angular-split": "^15.0.0",
    "client-app": "file:",
    "core-js": "3.38.1",
    "hammerjs": "^2.0.8",
    "keycharm": "^0.4.0",
    "lodash": "^4.17.21",
    "marked": "^14.1.0",
    "moment": "^2.30.1",
    "ng-zorro-antd": "^18.1.1",
    "ngx-captcha": "^13.0.0",
    "ngx-clipboard": "^16.0.0",
    "ngx-color-picker": "^17.0.0",
    "ngx-markdown": "^18.0.0",
    "ngx-progressbar": "^12.0.1",
    "ngx-vis": "^4.0.0",
    "propagating-hammerjs": "^3.0.0",
    "rxjs": "^7.8.1",
    "timsort": "^0.3.0",
    "tslib": "^2.7.0",
    "uuid": "^10.0.0",
    "vis-data": "^7.1.9",
