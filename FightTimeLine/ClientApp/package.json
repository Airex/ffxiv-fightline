    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@egjs/hammerjs": "^2.0.17",
    "@microsoft/signalr": "^6.0.25",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@sentry/browser": "7.119.0",
    "@xivapi/angular-client": "^2.0.1",
    "angular-split": "^15.0.0",
    "client-app": "file:",
    "core-js": "3.30.2",
    "hammerjs": "^2.0.8",
    "keycharm": "^0.4.0",
    "lodash": "^4.17.20",
    "marked": "^4.3.0",
    "moment": "^2.29.1",
    "ng-zorro-antd": "^15.1.1",
    "ngx-captcha": "^13.0.0",
